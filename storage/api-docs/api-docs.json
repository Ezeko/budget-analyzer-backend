{
    "openapi": "3.0.0",
    "info": {
        "title": "Budget Analyzer",
        "version": "1.0.1"
    },
    "paths": {
        "/api/budgets/{user_id}": {
            "get": {
                "tags": [
                    "Budgets"
                ],
                "summary": "Gets all user's budget",
                "description": "Create budgets ",
                "operationId": "Budgets",
                "responses": {
                    "422": {
                        "description": "Wrong credentials response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Sorry, wrong user details. Please try again"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "response": {
                                            "type": "string",
                                            "example": "success"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/history/create": {
            "post": {
                "tags": [
                    "History"
                ],
                "summary": "Create history",
                "description": "Create new budget history",
                "operationId": "History",
                "requestBody": {
                    "description": "Pass history credentials",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "user_id",
                                    "amount",
                                    "description",
                                    "budget_type"
                                ],
                                "properties": {
                                    "user_id": {
                                        "type": "string",
                                        "format": "id",
                                        "example": "flutW0MZyjQikx7mPpslUq37ryl1"
                                    },
                                    "amount": {
                                        "type": "number",
                                        "format": "number",
                                        "example": "12345"
                                    },
                                    "username": {
                                        "type": "string",
                                        "example": "ezekiel"
                                    },
                                    "description": {
                                        "type": "string",
                                        "example": "â‚¦20,000 was added to Education budget plan"
                                    },
                                    "budget_type": {
                                        "type": "string",
                                        "example": "Education"
                                    },
                                    "priority": {
                                        "type": "string",
                                        "example": "medium"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "422": {
                        "description": "Wrong credentials response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Sorry, wrong user details. Please try again"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "201": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "History Created Successfully"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
