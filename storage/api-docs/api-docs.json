        "/api/user/create": {
            "post": {
                "tags": [
                    "Users"
                ],
                "summary": "Create a user",
                "description": "Create new user ",
                "operationId": "newUser",
                "requestBody": {
                    "description": "Pass users credentials",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
